%!PS-Adobe-1.0
%%BoundingBox: 0 0 1400 600
%%BeginSetup
<< /PageSize [1400 600] /Orientation 0 >> setpagedevice
%%EndSetup

0 0 moveto
0 600 lineto
1400 600 lineto
1400 0 lineto
closepath
0 setgray
fill

% a1 .. a2
/rand_in_range {
    exch
    % a2 a1
    dup
    % a2 a1 a1
    3 1 roll
    % a1 a2 a1
    sub
    % a1 r
    rand
    16#7fffffff div
    exch
    mul
    add
} def

% R
/rand_in_circle {
    0 1 rand_in_range
    %dup mul
    dup 1 exch sub
    % R r 1-r

    % ^2
    %dup mul

    3 2 roll
    mul 
    % r R(1-r)
    
    0 360 rand_in_range dup
    % r R(1-r) a a
    sin
    % r R(1-r) a sin(a)
    2 index mul
    % r R(1-r) a y

    3 1 roll
    % ro y R(1-r) a
    cos
    % ro y R cos(a)
    mul
    % ro y x
    exch
    % ro x y
} def

% r x y
/set_color_light {
    400 div
    1.0 mul
    0.3 add
    % r x y
    
    exch
    1000 div
    0.4 mul
    exch
    % r x y

    dup
    % r x y y
    2 index
    % r x y y x
    mul
    % r x y xy
    0.1 mul
    0.3 add

    3 2 roll
    % r xy x y

    1 index add
    % r xy x (x+y)/2

    exch 
    % r xy (x+y)/2 x

    1 index
    % r xy (x+y)/2 x (x+y)/2
    
    div
    exch
    % r xy 2x/(x+y) (x+y)/2
    
    3 1 roll
    % r (x+y)/2 xy 2x/(x+y) 
    
    3 1 roll
    0 0.5 rand_in_range add
    3 1 roll
    0 0.5 rand_in_range add
    3 1 roll
    0 0.5 rand_in_range add

    setrgbcolor
} def

% r x y
/set_color_dark {
    400 div
    1.0 mul
    0.3 add
    % r x y
    
    exch
    1000 div
    0.4 mul
    exch
    % r x y

    dup
    % r x y y
    2 index
    % r x y y x
    mul
    % r x y xy
    0.1 mul
    0.3 add

    3 2 roll
    % r xy x y

    1 index add
    % r xy x (x+y)/2

    exch 
    % r xy (x+y)/2 x

    1 index
    % r xy (x+y)/2 x (x+y)/2
    
    div
    exch
    % r xy 2x/(x+y) (x+y)/2
    
    3 1 roll
    % r (x+y)/2 xy 2x/(x+y) 
    
    3 1 roll
    0 0.3 rand_in_range add
    3 1 roll
    0 0.3 rand_in_range add
    3 1 roll
    0 0.3 rand_in_range add

    setrgbcolor
} def

/circle {
    0 360 arc
    fill
} def

% x y n light_or_dark r_min r_max R
/splatter {
    7 5 roll
    % n light_or_dark r_min r_max R x y

    6 index 0 1
    % n light_or_dark r_min r_max R x y n 0 1

    fill
    3 2 roll { pop

        2 index rand_in_circle
        % ld rm r R x y ri dx dy

        3 index add
        % ld rm r R x y ri dx y2

        exch
        4 index add
        exch
        % ld rm r R x y ri x2 y2

        7 index
        % ld rm r R x y ri x2 y2 rm

        7 index
        % ld rm r R x y ri x2 y2 rm r
        4 index
        % ld rm r R x y ri x2 y2 rm r ri
        mul
        % ld rm r R x y ri x2 y2 rm r
        4 index
        mul
        4 index
        mul
        4 index
        mul
        % ld rm r R x y ri x2 y2 rm r

        rand_in_range
        % ld rm r R x y ri x2 y2 r
    

        2 index
        2 index
        % ld rm r R x y ri x2 y2 r x2 y2

    11 index
    0 eq {
            set_color_dark
    } {
            set_color_light
    } ifelse

        circle
    pop
    } for
    moveto
    pop pop pop pop pop
} def

